[
    {
        "name": "${application}",
        "image": "${image_url}",
        "essential": true,
        "portMappings": [
             {
                "hostPort": 8000,
                "containerPort": 8000,
                "protocol": "tcp"
             }
         ],
         "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "${cloudwatch_group}",
            "awslogs-region": "${region}",
            "awslogs-stream-prefix": "${application}"
          }
        },
        "links": [],
        "command": [],
        "entryPoint": []
    }
]